                                                @{
                                                    List<SelectListItem> listregion = new List<SelectListItem>();
                                                    listregion.Add(new SelectListItem
                                                    {
                                                        Text = "England Wales",
                                                        Value = "England Wales"
                                                    });
                                                    listregion.Add(new SelectListItem
                                                    {
                                                        Text = "Scotland",
