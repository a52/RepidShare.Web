@model List<RepidShare.Entities.ActivityModel>




@if (Model != null && Model.Count > 0)
{
    for (int i = 0; i < Model.Count; i++)
    {
    <div class="row">
        <div class="col-sm-3 activity_date">
            <h6>@Model[i].CreatedOn.ToString("dd MMM yyyy hh:mm")</h6>
        </div>
        <div class="col-sm-9 activity_audit">
            <span>
                @Model[i].ActivityText
            </span>
        </div>
    </div>
    }
}


